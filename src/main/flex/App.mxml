<?xml version="1.0" encoding="utf-8"?>
<js:Application xmlns:fx="http://ns.adobe.com/mxml/2009"
				xmlns:js="library://ns.apache.org/royale/basic"
				xmlns:local="*"
				xmlns:view="org.ro.view.*"
				pageTitle="HelloWorld">
			   
	<fx:Declarations>
	</fx:Declarations>
	
	<fx:Script>
<![CDATA[
        [Embed('../resources/images/bars.png')]
        [Bindable]
        public var MenuIcon:Class;

        [Embed("../resources/images/times_red.png")]
        [Bindable]
        public var TimesRedIcon:Class;

        [Embed("../resources/images/times_yellow.png")]
        [Bindable]
        public var TimesYellowIcon:Class;

        [Embed("../resources/images/step-forward.png")]
        [Bindable]
        public var StepForwardIcon:Class;

        [Embed("../resources/images/step-backward.png")]
        [Bindable]
        public var StepBackwardIcon:Class;

        [Embed("../resources/images/eye.png")]
        [Bindable]
        public var EyeIcon:Class;

        [Embed("../resources/images/check.png")]
        [Bindable]
        public var CheckIcon:Class;

        [Embed("../resources/images/eye-slash.png")]
        [Bindable]
        public var EyeSlashIcon:Class;

        [Embed("../resources/images/tree.png")]
        [Bindable]
        public var TreeIcon:Class;


        	]]>
	</fx:Script>
	
	<js:valuesImpl>
		<js:SimpleCSSValuesImpl />
	</js:valuesImpl>
	
	<js:initialView>
		<js:View width="100%" height="100%">
	        <js:VGroup>
			</js:VGroup>
			<js:Container width="100%" height="100%" >
				<js:Label id="lbl" text="Hello World" width="200" />
			</js:Container>	
		</js:View>
	</js:initialView>
	
 </js:Application>